          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n\n    CAST(DATE_FORMAT(o.created_at, '%Y-%m-%d %H:00:00') AS DATETIME) AS `hour`,\n\n\n\n    SUM(o.size * p.last_px) AS `total_volume_in_usd`\n\nFROM\n\n    order_execution_log AS o\n\nJOIN\n\n    (SELECT symbol, last_px\n\n     FROM stream_data\n\n     WHERE (symbol, timestamp) IN\n\n     (\n\n         SELECT symbol, MAX(timestamp)\n\n         FROM stream_data\n\n         GROUP BY symbol\n\n     )) AS p\n\nON\n\n    o.product_id = p.symbol\n\nWHERE\n\n    o.product_id IN ('BTC/USDC:USDC', 'ETH/USDC:USDC')\n\nGROUP BY\n\n    `hour`\n\nORDER BY\n\n    `hour` ASC; ",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [
                  {
                    "name": "*",
                    "type": "functionParameter"
                  }
                ],
                "type": "function"
              }
