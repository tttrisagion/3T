# This is an example secrets file for Hyperliquid exchange using the ccxt library.
# DO NOT COMMIT THE ACTUAL secrets.yml FILE TO GIT.
# Copy this file to secrets.yml and replace the placeholder values with your actual credentials.

hyperliquid:
  apiKey: YOUR_API_KEY
  secret: YOUR_WALLET_PRIVATE_KEY # This is the private key for your wallet
  walletAddress: YOUR_PUBLIC_WALLET_ADDRESS
